     <RmGLShader NAME="Fragment Program" API="OpenGL" PIXEL_SHADER="TRUE"><RmShaderCode TYPE="TEXT"><![CDATA[
uniform sampler2D baseMap;
uniform float pixelSize;

varying vec2 Texcoord;

vec4 texSmoothing (sampler2D Texture0, vec2 uv)
{
   vec4 smoothedCol = vec4(0.0); 
   for (int i=-2; i<=2; i++)
   {
      for (int j=-2; j<=2; j++)
      {
         smoothedCol += texture2D(Texture0,  uv + vec2(i, j) * pixelSize );
      }
    }
    return smoothedCol/25.0;
}

void main(void)
{
    gl_FragColor = texSmoothing( baseMap, Texcoord );
}]]></RmShaderCode></RmGLShader>
