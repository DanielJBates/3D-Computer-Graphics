uniform sampler2D baseMap;

varying vec2 Texcoord;
varying float fadingRate;

void main( void )
{
   float x = Texcoord.x; 
   float y = Texcoord.y; 
   float range = 0.5 * 0.5 - dot(Texcoord - 0.5, Texcoord - 0.5);
   float shade = 2 / (1 + exp(12 * range));   
   gl_FragColor = (1 - shade) * texture2D( baseMap, Texcoord ) * fadingRate;
}]]></RmShaderCode></RmGLShader>
     <RmGLShader NAME="Vertex Program" API="OpenGL" PIXEL_SHADER="FALSE"><RmShaderCode TYPE="TEXT"><![CDATA[
uniform mat4 viewInverse;

uniform float v;
uniform float billboardSize;
uniform float freq;
uniform float lifecycle;
uniform float time;
uniform float particleSpread;
uniform float particleSystemHeight;

varying vec2 Texcoord;
varying float fadingRate;

void main( void )
{
    vec4 bbPos = gl_Vertex;
    bbPos.xy *= billboardSize;
    
    vec3 Vx = viewInverse[0].xyz;    
    vec3 Vy = viewInverse[1].xyz; 
    bbPos.xyz = (bbPos.x * Vx + bbPos.y * Vy).xyz;
     
    float T = mod(freq * time, lifecycle);
    
    bbPos.x += particleSpread * T * cos(300 * gl_Vertex.z);
    bbPos.z += particleSpread * T * sin(300 * gl_Vertex.z);
    bbPos.y += particleSystemHeight * T; 
    
    fadingRate = (1.0 -T/lifecycle);
    
    Texcoord = sign(bbPos.xy) * 0.5 + 0.5;
    
    gl_Position = gl_ModelViewProjectionMatrix * bbPos;    
}]]></RmShaderCode></RmGLShader>
