{
   vec4 pos = gl_Vertex;
   
   if((pos.y > 6 && pos.z < -5 && pos.x > -1) || (pos.y < -10 && pos.z < -4 && pos.x < -8.5))
    {
       pos.x -= 7.5 * sin(time * 20);
    }
    
   if((pos.y < -8 && pos.z < -5 && pos.x > -8.5) || (pos.y > 6 && pos.z < -4 && pos.x < -20))
    {
       pos.x += 7.5 * sin(time * 20);
    }
   
   pos.xyz *= 0.05;
   
   mat2 RotByX=mat2(cos(4.71239), sin(4.71239), -sin(4.71239), cos(4.71239));
   pos.yz = RotByX * pos.yz;
   
   pos.y -= 13.90;
   pos.z += 10;
   
   float angle = -6.28319 * (time * 0.2);

   mat2 RotByY = mat2(cos(angle), -sin(angle), sin(angle), cos(angle));
   pos.xz = pos.xz * RotByY;
   
   Texcoord = gl_MultiTexCoord0.xy;
    
   vec4 fvObjectPosition = gl_ModelViewMatrix * gl_Vertex;
   
   ViewDirection = -fvObjectPosition.xyz;
   LightDirection = fvLightPosition - gl_Vertex.xyz;
   Normal = gl_NormalMatrix * gl_Normal;
   
   gl_Position = gl_ModelViewProjectionMatrix * pos;
