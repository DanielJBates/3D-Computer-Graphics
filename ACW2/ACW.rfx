     <RmGLShader NAME="Fragment Program" API="OpenGL" PIXEL_SHADER="TRUE"><RmShaderCode TYPE="TEXT"><![CDATA[
uniform sampler2D FireShape;
uniform sampler2D FireNoise;
uniform sampler2D FireBase;

uniform float time;
uniform vec2 freq;

varying vec2 TexCoord;

void main(void)
{
   vec2 tex = TexCoord;
   tex.y = 1.0 - tex.y;
   
   tex -= vec2(0.5, 0);
   tex *= vec2(0.7, 0.7);
   tex += vec2(0.5, 0);
   
   tex.x += 0.1 * tex.y * sin(freq.x * (tex.y + time));
   tex.y += 0.1 * tex.y * sin(freq.y * (tex.x + time));
   
   vec4 fireShape = texture2D(FireShape, tex);
   vec4 fireNoise = texture2D(FireNoise, (1 - TexCoord) - 0.9 * time);
   vec4 fireColour = texture2D(FireBase, 1 - TexCoord); 
   
   gl_FragColor = fireShape * fireNoise * fireColour;
}]]></RmShaderCode></RmGLShader>
